#/bin/bash

micro80/convert_png_to_micro80/convert_png_to_micro80 micro80

../../c8080 -OCPM -DARCH_MICRO80_COLOR -Imicro80 -olines.micro80.com \
 lines.c \
 music.c \
 path.c \
