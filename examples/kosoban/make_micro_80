#/bin/bash

micro80/convert_png_to_micro80/convert_png_to_micro80 micro80

../../c8080 -OCPM -DARCH_MICRO80_COLOR -Imicro80 -okosoban.micro80.com kosoban.c levels.c micro80/resources.c

if [ -e  /home/alemorf/a/work/micro80_emulator/test.com ]
then
    cp kosoban.micro80.com /home/alemorf/a/work/micro80_emulator/test.com
    echo Make /home/alemorf/a/work/micro80_emulator/test.com
    cd /home/alemorf/a/work/micro80_emulator && make
fi

