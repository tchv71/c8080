#/bin/bash
# set -e
platform=$(uname -o)
EXT=
ASM=
if [ "$platform" == "Darwin" ]; then
    EXT=.macos
    ASM=-A../../include/sjasmplus.macos
fi

make -C specialist/convert_png_to_specialist convert_png_to_specialist$EXT

specialist/convert_png_to_specialist/convert_png_to_specialist$EXT specialist

../../c8080$EXT $ASM -ORKS -DARCH_SPECIALIST -Ispecialist -okosoban.specialist.rks \
 kosoban.c \
 levels.c \
 specialist/resources.c
